<suki:SukiWindow xmlns="https://github.com/avaloniaui"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:suki="https://github.com/kikipoulet/SukiUI"
                 xmlns:views="clr-namespace:ChatClient.Desktop.Views"
                 MinWidth="300"
                 MinHeight="300"
                 Opacity="0"
                 x:Class="ChatClient.Desktop.Views.SukiDialogWindow"
                 TitleBarAnimationEnabled="False"
                 Title="SukiDialogWindow">
    <suki:SukiWindow.Transitions>
        <Transitions>
            <DoubleTransition Property="Opacity" Duration="0:0:0.4" Easing="CubicEaseOut" />
        </Transitions>
    </suki:SukiWindow.Transitions>
</suki:SukiWindow>